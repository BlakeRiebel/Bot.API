{
  "LoopTimes": 5,
  "WebApiDatabase": "",
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "MySQLServer",
        "Args": {
          "connectionString": "",
          "sinkOptionsSection": {
            "tableName": "EventLog",
            "autoCreateSqlTable": false,
            "batchPostingLimit": 1000,
            "period": "0.00:00:30"
          },
          "columnOptionsSection": {
            "disableTriggers": true,
            "clusteredColumnstoreIndex": false,
            "addStandardColumns": [ "LogEvent" ],
            "additionalColumns": [
              {
                "ColumnName": "Message",
                "PropertyName": "Message",
                "DataType": "varchar",
                "DataLength": 250,
                "NonClusteredIndex": false
              },
              {
                "ColumnName": "Exception",
                "PropertyName": "Exception",
                "DataType": "varchar",
                "DataLength": 500,
                "NonClusteredIndex": false
              },
              {
                "ColumnName": "Information",
                "PropertyName": "Information",
                "DataType": "varchar",
                "DataLength": 500,
                "NonClusteredIndex": false
              }
            ],
            "level": {
              "columnName": "Severity",
              "storeAsEnum": true
            },
            "timeStamp": {
              "columnName": "Timestamp",
              "convertToUtc": true
            },
            "logEvent": {
              "excludeAdditionalProperties": true,
              "excludeStandardColumns": true
            },
            "message": { "columnName": "Message" },
            "exception": { "columnName": "Exception" }
          }
        }
      }
    ]
  }
}